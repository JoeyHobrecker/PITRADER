# .env.example: Copy to .env and fill in your credentials
#
# This file is loaded by Docker Compose to securely inject secrets
# into the containers as environment variables.

# Alpaca Paper Trading API
ALPACA_KEY="YOUR_ALPACA_KEY"
ALPACA_SECRET="YOUR_ALPACA_SECRET"

# OpenAI API
OPENAI_API_KEY="YOUR_OPENAI_KEY"

# Telegram Bot
TELEGRAM_BOT_TOKEN="YOUR_BOT_TOKEN_FROM_BOTFATHER"
TELEGRAM_CHAT_ID="YOUR_NUMERIC_CHAT_ID" # Get this from a bot like @userinfobot

# Weaviate Hosted Vector Store
WEAVIATE_URL="https://your-weaviate-cluster.weaviate.network"
WEAVIATE_API_KEY="YOUR_WEAVIATE_API_KEY"
